{
  "expo": {
    "name": "ShareCard",
    "slug": "ShareCard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_4.png",
    
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_4.png",
        "backgroundColor": "#2d2c47"
      },
      "package": "com.anirudh.j.sharecard"
    },
    "web": {
      "favicon": "./assets/logo_1.png"
    },
    "plugins": [
      "expo-document-picker"
    ]
  }
}
